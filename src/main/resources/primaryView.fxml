<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.PrimaryController">
    <GridPane layoutX="50.0" layoutY="134.0" prefWidth="300.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Button mnemonicParsing="false" onAction="#openWindowClients" prefHeight="26.0" prefWidth="96.0"
                text="Clients"/>
        <Button mnemonicParsing="false" onAction="#openWindowProducts" prefWidth="96.0" text="Products"
                GridPane.columnIndex="1"/>
        <Button mnemonicParsing="false" onAction="#openWindowOrders" prefWidth="96.0" text="Orders"
                GridPane.columnIndex="2"/>
    </GridPane>
</AnchorPane>
